generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model Post {
  id        String   @id
  content   String   
  createdAt DateTime @default(now())
  author    String

  @@index([author])
}

model Champs{
    name String @id
    elo Int 

    @@index([name])
}